{
  "name": "next-graphql-prisma",
  "workspaces": {
    "packages": [
      "./services/*"
    ]
  },
  "cacheDirectories": [
    ".yarn/cache",
    "services/front/.next/cache"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -pi run build",
    "start": "yarn workspaces foreach -pi run start",
    "dev": "yarn workspaces foreach -pi run dev"
  },
  "npmClient": "yarn",
  "packageManager": "yarn@3.6.3"
}
