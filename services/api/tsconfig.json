{
  "references": [],
  "compilerOptions": {
    "composite": false,
    "target": "es2019",
    "lib": [
      "dom",
      "ES2020"
    ],
    "module": "commonjs",
    "noErrorTruncation": true,
    "allowJs": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "./src",
    "rootDir": "./src",
    "sourceMap": true,
    "noUncheckedIndexedAccess": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "incremental": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
    "typeRoots": [
      "./src/types/",
      "node_modules/@types/",
      "../../node_modules/@types/"
    ],
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/generated",
    "_test.ts",
    "src/types"
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}